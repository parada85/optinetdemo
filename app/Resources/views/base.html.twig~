				})

				$("#enviarpass").live('click', function () {
						var mail = $("#mail").val();
						url = Routing.generate('enviarmailpass', { mail: mail });
						$.ajax({url: url});
						alert("Se ha enviado un nuevo mail con la contrase√±a.Gracias")

				})


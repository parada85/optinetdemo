				$("#guardarreserva").live('click', function () {
						var input = $("#adelanto").val();
						var contador = tabla4.fnSettings().fnRecordsTotal();
						
						if (isNaN(input) || contador == 0 || input == "" || input < 0 )
							$("#error").dialog('open');
						else
							$("#confirmar8").dialog('open');
				})
				
